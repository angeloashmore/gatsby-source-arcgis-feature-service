{
  "name": "gatsby-source-arcgis-feature-service",
  "version": "0.2.1",
  "description": "Gatsby source plugin for building websites using an ArcGIS Feature Service as a data source",
  "scripts": {
    "build": "NODE_ENV=production rollup -c",
    "build:watch": "npm run build -- --watch",
    "format": "prettier --write 'src/**/*.js' 'rollup.config.js' 'docs/**/*.md'",
    "prebuild": "rimraf dist",
    "prepare": "npm run build",
    "release": "standard-version"
  },
  "homepage": "https://github.com/angeloashmore/gatsby-source-arcgis-feature-service",
  "repository": {
    "type": "git",
    "url": "https://github.com/angeloashmore/gatsby-source-arcgis-feature-service.git"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-plugin",
    "arcgis",
    "gis",
    "geojson"
  ],
  "author": "Angelo Ashmore <angelo.ashmore@walltowall.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/preset-env": "^7.5.5",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.2.2",
    "husky": "^3.0.4",
    "prettier": "^1.18.2",
    "rollup": "^1.20.3",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-clear": "^2.0.7",
    "standard-version": "^7.0.0"
  },
  "dependencies": {
    "got": "^9.6.0",
    "polylabel": "^1.0.2"
  },
  "peerDependencies": {
    "gatsby": ">=2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
