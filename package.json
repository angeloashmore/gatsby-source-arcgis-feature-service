{
  "name": "gatsby-source-arcgis-feature-service",
  "version": "0.1.3",
  "description": "Gatsby source plugin for building websites using an ArcGIS Feature Service as a data source",
  "scripts": {
    "build": "NODE_ENV=production rollup -c",
    "build:watch": "npm run build -- --watch",
    "format": "prettier --write 'src/**/*.js' 'rollup.config.js' 'docs/**/*.md'",
    "prebuild": "rimraf dist",
    "prepare": "npm run build",
    "release": "standard-version"
  },
  "homepage": "https://github.com/angeloashmore/gatsby-source-arcgis-feature-service",
  "repository": {
    "type": "git",
    "url": "https://github.com/angeloashmore/gatsby-source-arcgis-feature-service.git"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-plugin",
    "arcgis",
    "gis",
    "geojson"
  ],
  "author": "Angelo Ashmore <angelo.ashmore@walltowall.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "babel-eslint": "^10.0.2",
    "eslint": "^6.2.1",
    "husky": "^1.3.1",
    "prettier": "^1.16.4",
    "rollup": "^1.19.4",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-clear": "^2.0.7",
    "standard-version": "^5.0.1"
  },
  "dependencies": {
    "gatsby-node-helpers": "^0.3.0",
    "got": "^9.6.0",
    "ramda": "^0.26.1"
  },
  "peerDependencies": {
    "gatsby": ">=2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
